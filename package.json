{
  "name": "nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "set NODE_ENV=development && nest start",
    "start:dev": "set NODE_ENV=development && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "prebuild": "rimraf dist",
    "prettier-fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "eslint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "create-moudle": "nest g mo module/posts",
    "create-controller": "nest g co module/posts",
    "create-child-controller": "nest g co module/example/example-test --no-spec --flat",
    "create-service": "nest g s module/posts",
    "create-filter": "nest g filter filter/http-exception",
    "create-interceptor": "nest g interceptor interceptor/transform",
    "create-middleware": "nest g middleware logger middleware",
    "create-guard": "nest g gu jwt guard",
    "create-pipe": "nest g pi name pipe"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.6",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/schedule": "^2.0.1",
    "@types/express-session": "^1.17.4",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cache-manager": "^4.0.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.3",
    "express-rate-limit": "^6.4.0",
    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.4",
    "nest-winston": "^1.6.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "typeorm": "^0.2.41",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/testing": "^8.4.6",
    "@nestjs/typeorm": "^8.1.2",
    "@types/cache-manager": "^4.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.42",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "swagger-ui-express": "^4.4.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
